datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Todo {
  id      Int    @id @default(autoincrement())
  title   String
  body    String
  done    Boolean
  createdAt DateTime @default(now())
}
